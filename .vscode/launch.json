    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "baidu",
                "-i", "${workspaceFolder}/example.xcstrings",
                "--app-id", "${env:BAIDU_APP_ID}",
                "--app-secret", "${env:BAIDU_APP_SECRET}",
                "-s", "en",
                "-t", "fr",
                "-t", "de"
            ]
        }
    ]
